  "bugs": {
    "url": "https://github.com/nickzuber/levenshtein-lite/issues"
  },
  "homepage": "https://github.com/nickzuber/levenshtein-lite#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.11.1",
    "knuth-shuffle": "^1.0.1",
    "tape": "^4.5.1",
    "winston": "^2.2.0",
    "word-list": "^1.0.1"
